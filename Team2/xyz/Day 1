https://leetcode.com/problems/majority-element/submissions/

class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n = nums.size();
        int majority = n/2;
        unordered_map<int,int>freq;
        for(int i = 0;i<n;i++){
            freq[nums[i]]++;
            if (freq[nums[i]] > majority){
                return nums[i];
            }
        }
        return false;
    }

    
};

