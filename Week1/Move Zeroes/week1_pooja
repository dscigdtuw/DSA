class Solution {
public:
    void moveZeroes(vector<int>& nums) {
      
        int zeroFound = 0;
        
        for(int i = 0; i < nums.size(); i++)
        {
            if(nums[i] != 0)
            {
                nums[zeroFound++] = nums[i];
            
            }
        }
        for(int i = zeroFound; i < nums.size(); i++)
            nums[i] = 0;
    }
};
